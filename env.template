# AI Agents System Environment Variables

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# LangSmith Configuration (optional, for monitoring)
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# Database Configuration (will be overridden by Docker Compose)
DATABASE_URL=postgresql://ai_user:ai_password@localhost:5432/ai_agents

# Redis Configuration (will be overridden by Docker Compose)
REDIS_URL=redis://localhost:6379

# Application Configuration
LOG_LEVEL=INFO
ENVIRONMENT=development 